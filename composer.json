{
    "name": "simpleteam/azure-active-directory-oauth",
    "description": "This is an OAuth provider for Azure Active Directory. It is supposed to be used with Social plugin from Dukt",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "azure active directory oauth"
    ],
    "support": {
        "docs": "https://simple.com.au",
        "issues": "https://simple.com.au"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jinzhe Li",
            "homepage": "https://simple.com.au"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "craftcms/cms": "^3.0.20 | ^4.0",
        "thenetworg/oauth2-azure": "^2.0",
        "dukt/social": "^2.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "simpleteam\\azureactivedirectoryoauth\\": "src/"
        }
    },
    "extra": {
        "name": "Azure Active Directory OAuth",
        "handle": "azure-active-directory-oauth",
        "schemaVersion": "0.0.1",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "",
        "class": "simpleteam\\azureactivedirectoryoauth\\AzureActiveDirectoryOauth"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "craftcms/phpstan": "dev-main",
        "craftcms/rector": "dev-main"
    }
}
