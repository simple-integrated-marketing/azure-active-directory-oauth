{
    "name": "simpleteam/azure-active-directory-oauth",
    "description": "This is an OAuth provider for Azure Active Directory. It is supposed to be used with Social plugin from Dukt",
    "type": "craft-plugin",
    "version": "0.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "azure active directory oauth"
    ],
    "support": {
        "docs": "https://simple.com.au",
        "issues": "https://simple.com.au"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jinzhe Li",
            "homepage": "https://simple.com.au"
        }
    ],
    "require": {
        "craftcms/cms": "~3.0.0-beta.23",
        "thenetworg/oauth2-azure": "^1.3"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
          "simpleteam\\azureactivedirectoryoauth\\": "src/"
        }
    },
    "extra": {
        "name": "Azure Active Directory OAuth",
        "handle": "azure-active-directory-oauth",
        "schemaVersion": "0.0.1",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "",
        "class": "simpleteam\\azureactivedirectoryoauth\\AzureActiveDirectoryOauth"
    }
}
